    agent any 
    stages {
        stage('Build') { 
            steps {
                      echo ' building the aoolication ...!'
            }
        }
        stage('Test') { 
            steps {
                echo 'testing the application...!'
            }
        }
        stage('Deploy') { 
            steps {
          echo 'deploying the application'...!
            }
        }
    }
}
